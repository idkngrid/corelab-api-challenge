// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Vehicle {
  id          Int @id @default(autoincrement())
  title       String
  description String
  color       String
  year        Int
  plate       String
  price       Float
  isFavorite  Boolean
}
